r\n    #nav-list li a p{\r\n\r\n      font-family: 'Bungee', cursive;\r\n      padding-left: 5px;\r\n      padding-top: 1px;\r\n      color: #ffffff;\r\n      display: flex;\r\n      flex-direction: column;\r\n      font-size: 15px;\r\n      transition: 0.3s;\r\n      opacity: 0;\r\n      position: absolute;\r\n    }\r\n    #index-fnt:hover i{\r\n      opacity: 0;\r\n      transition: 0.3s;\r\n    }\r\n    #index-fnt:hover p{\r\n      opacity: 1;\r\n    }\r\n    #index-fnt p{\r\n      top: 100px;\r\n      left: 13px;\r\n    }\r\n\r\n    #about-fnt:hover i{\r\n      opacity: 0;\r\n      transition: 0.3s;\r\n    }\r\n    #about-fnt:hover p{\r\n      opacity: 1;\r\n    }\r\n    #about-fnt p{\r\n      top: 175px;\r\n      left: 8px;\r\n    }\r\n\r\n\r\n\r\n    #work-fnt:hover i{\r\n      opacity: 0;\r\n      transition: 0.3s;\r\n    }\r\n    #work-fnt:hover p{\r\n      opacity: 1;\r\n    }\r\n    #work-fnt p{\r\n      top: 248px;\r\n\r\n      left: 12px;\r\n    }\r\n\r\n    #contact-fnt:hover i{\r\n      opacity: 0;\r\n      transition: 0.3s;\r\n    }\r\n    #contact-fnt:hover p{\r\n      opacity: 1;\r\n    }\r\n    #contact-fnt p{\r\n      top: 321px;\r\n      left: 0px;\r\n    }\r\n\r\n    #linkedin-fnt:hover i{\r\n      transition: 0.3s;\r\n      color: #ffffff;\r\n    }\r\n  }\r\n\r\n}\r\n#myLogo{\r\n  position: absolute;\r\n  right: 15px;\r\n  top: 15px;\r\n}\r\n\r\n#nav-list{\r\n  align-items: center;\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n\r\n  li{\r\n    margin-top: 50px;\r\n    list-style: none;\r\n  }\r\n  a{\r\n    text-decoration: none;\r\n  }\r\n  img{\r\n    background-color: #FFFFFF;\r\n    border-style: solid;\r\n    border-color: #000000;\r\n    height: 50px;\r\n    width: 50px;\r\n\r\n  }\r\n}\r\n.home-content{\r\n  background-color: linear-gradient(to bottom, #ff6666 0%, #ccff66 100%);\r\n  #canvas{\r\n    background-color: white;\r\n    height: 100vh;\r\n    width: 100%;\r\n  }\r\n}\r\n.about-content{\r\n  font-family: 'Bungee', cursive;\r\n  #canvas{\r\n    height: 100vh;\r\n    width: 100%;\r\n  }\r\n  #description{\r\n    position: absolute;\r\n    color: #FFFFFF;\r\n    background-color: hsla(0, 0%, 0%, 0.76);\r\n    padding: 10px 10px;\r\n    left: 200px;\r\n    top: 100px;\r\n\r\n  }\r\n}\r\n.work-content{\r\n  #canvas{\r\n    height: 100vh;\r\n    width: 100%;\r\n  }\r\n  .buttons{\r\n    position: absolute;\r\n    display: flex;\r\n    width: 100%;\r\n    top: 50%;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    z-index: 10;\r\n    button{\r\n      outline: none;\r\n\r\n      border: none;\r\n      background: none;\r\n      height: 40px;\r\n      margin: 0px 35%;\r\n    }\r\n    i{\r\n      color: #ffffff;\r\n    }\r\n  }\r\n  #left-btn:hover i{\r\n    transition: 0.3s;\r\n    color: #f42442;\r\n  }\r\n  #right-btn:hover i{\r\n    transition: 0.3s;\r\n    color: #f42442;\r\n  }\r\n\r\n\r\n  #card_container{\r\n    top: 70px;\r\n    left: 0;\r\n    position: absolute;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    width: 100%;\r\n    flex-direction: column;\r\n\r\n    div{\r\n      border-radius: 0px 0px 30px 30px;\r\n      background-color: rgba(0, 0, 0, 0.50);\r\n      position: relative;\r\n      z-index: 5;\r\n      word-wrap: break-word;\r\n      width: 50%;\r\n      padding: 10px;\r\n      height: 250px;\r\n\r\n      p{\r\n        text-align: center;\r\n        color: #ffffff;\r\n        font-size: 35px;\r\n      }\r\n\r\n      a{\r\n        display: flex;\r\n        justify-content: center;\r\n        color: #ffffff;\r\n        font-size: 35px;\r\n        text-decoration: none;\r\n      }\r\n    }\r\n    button{\r\n      width: 100px;\r\n      position: relative;\r\n      left: 25%;\r\n      top: 15px;\r\n      padding: 10px;\r\n      border: none;\r\n      background: none;\r\n      cursor: pointer;\r\n    }\r\n    #card_exit{\r\n      position: relative;\r\n      z-index: 20;\r\n      outline: none;\r\n      box-shadow: none;\r\n      i{\r\n        color: #ffffff;\r\n      }\r\n    }\r\n    #card_exit:hover i{\r\n      transition: 0.3s;\r\n      color: #f42442;\r\n    }\r\n\r\n\r\n    #card_image{\r\n      border-radius: 30px 30px 0px 0px;\r\n      position: relative;\r\n      padding: 10px;\r\n      width: 50%;\r\n      object-fit: cover;\r\n      object-position: 50% 25%;\r\n      height: 300px;\r\n      background-color: rgba(0, 0, 0, 0.50);\r\n    }\r\n  }\r\n\r\n  #card_container:hover div a i{\r\n    transition: 0.3s;\r\n    color: #fcba03;\r\n  }\r\n\r\n}\r\n\r\n.home, .about, .work{\r\n  position: absolute;\r\n  z-index: 1;\r\n  background-color: grey;\r\n  height: 100%;\r\n  width: 100%;\r\n\r\n  .black-screen{\r\n    height: 100vh;\r\n    width: 100vw;\r\n    position: absolute;\r\n    z-index: 50;\r\n    background-color: white;\r\n    opacity: 0;\r\n    display: none;\r\n  }\r\n\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"e4745540344d4814c41514964002a82f","cacheData":{"env":{}}}